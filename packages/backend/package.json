{
  "name": "@linos/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build && node copy-assets.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "check-db": "ts-node src/scripts/check-database-integrity.ts",
    "start:prod": "yarn build && yarn check-db && node dist/main.js 2>&1 | grep -v 'bare-fs' | grep -v 'bare-os' | grep -v 'prisma.*collides'",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "prebuild": "yarn prisma:generate",
    "pretest": "yarn prisma:generate",
    "test": "ts-node scripts/setup-test-env.ts && jest --coverage",
    "test:watch": "ts-node scripts/setup-test-env.ts && jest --watch",
    "test:debug": "ts-node scripts/setup-test-env.ts && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "ts-node scripts/setup-test-env.ts && jest --config jest.integration.config.js",
    "test:e2e": "ts-node scripts/setup-test-env.ts && jest --config jest.e2e.config.js",
    "test:all": "ts-node scripts/setup-test-env.ts && yarn test && yarn test:integration && yarn test:e2e",
    "clean": "rimraf dist",
    "seed": "node dist/scripts/seed.js",
    "pretest:integration": "DATABASE_URL=file:./test.db yarn prisma migrate deploy",
    "pretest:e2e": "DATABASE_URL=file:./test.db yarn prisma migrate deploy",
    "setup-test-env": "ts-node scripts/setup-test-env.ts",
    "generate-prisma": "prisma generate",
    "prisma:validate": "prisma validate --schema=prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prepare:prod": "yarn prisma generate && yarn prisma migrate deploy && yarn build",
    "start:prod:cluster": "pm2 start ecosystem.config.js --env production",
    "start:prod:cluster:dev": "pm2 start ecosystem.config.js --env development",
    "start:prod:cluster:staging": "pm2 start ecosystem.config.js --env staging",
    "start:prod:cluster:prod": "pm2 start ecosystem.config.js --env production",
    "cloud:backup": "ts-node src/scripts/backup/cloud-backup.ts",
    "cloud:setup": "npx prisma generate && npx prisma migrate deploy",
    "cloud:init": "npx prisma generate && npx prisma migrate deploy && node dist/scripts/seed.js",
    "cloud:setup-static": "node ../../scripts/cloud/setup-static.js",
    "cloud:full-setup": "node ../../scripts/cloud/full-setup.js",
    "deploy:clean": "prisma migrate deploy && node dist/scripts/seed.js",
    "postinstall": "node scripts/copy-logo.js && npx puppeteer browsers install chrome"
  },
  "prisma": {
    "seed": "node dist/scripts/seed.js"
  },
  "dependencies": {
    "@linos/shared": "*",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@prisma/client": "5.22.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.3",
    "puppeteer": "^22.8.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.14.1",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "pdf-parse": "^1.1.1",
    "pm2": "^5.2.0",
    "prisma": "5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
